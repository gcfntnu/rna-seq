libprep_config:
  'Lexogen SENSE mRNA-Seq Library Prep Kit V2':
    strand: reverse
    single_end:
      adapter: 'AGATCGGAAGAGCACACGTCTGAACTCCAGTCA'
      trimmer: fastp
      fastp:
        params: '-f 10 -l 20'
    paired_end:
      adapter: 'AGATCGGAAGAGCACACGTCTGAACTCCAGTC'
      adapter2: 'AGATCGGAAGAGCGTCGTGTAGGGAAAGAGTGT' 
      trimmer: fastp
      fastp:
        params: '-f 10 -F 7 -l 20'
  'Lexogen SENSE Total RNA-Seq Library Prep Kit (w/RiboCop rRNA Depletion Kit V1.2)':
    strand: reverse
    single_end:
      adapter: 'AGATCGGAAGAGCACACGTCTGAACTCCAGTCA'
      trimmer: fastp
      fastp:
        params: '-f 10 -l 20'
    paired_end:
      adapter: 'AGATCGGAAGAGCACACGTCTGAACTCCAGTC'
      adapter2: 'AGATCGGAAGAGCGTCGTGTAGGGAAAGAGTGT'      
      trimmer: fastp
      fastp:
        params: '-f 10 -F 7 -l 20'
  'Illumina TruSeq Stranded mRNA Library Prep':
    strand: reverse
    single_end:
      adapter: 'AGATCGGAAGAGCACACGTCTGAACTCCAGTC'
      trimmer: fastp
      fastp:
        params: '-t 1'
    paired_end:
      adapter: 'AGATCGGAAGAGCACACGTCTGAACTCCAGTC'
      adapter2: 'AGATCGGAAGAGCGTCGTGTAGGGAAAGAGTGT'  
      trimmer: fastp
      fastp:
        params: '-t 1 -T 1'